{
  "name": "jw-presenter",
  "productName": "JW Presenter",
  "version": "1.0.0",
  "description": "Seamlessly present media designed for Jehovah's Witnesses.",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "dist-test": "electron-builder --dir",
    "build-all": "electron-builder --mac --win",
    "build-mac": "electron-builder --mac",
    "build-win": "electron-builder --win"
  },
  "author": {
    "name": "Christopher West",
    "url": "http://cwestify.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/westc/jw-presenter.git"
  },
  "license": "ISC",
  "dependencies": {
    "musicmetadata": "^2.0.5",
    "showdown": "^1.6.4",
    "electron-updater": "^2.4.3",
    "fs-extra": "^4.0.1"
  },
  "devDependencies": {
    "electron": "^1.6.6",
    "electron-builder": "^19.11.1"
  },
  "build": {
    "appId": "com.westc.jwpresenter",
    "mac": {
      "target": [
        "zip"
      ],
      "icon": "assets/icons/JWPresenter.icns"
    },
    "win": {
      "target": [
        "zip"
      ],
      "icon": "assets/icons/JWPresenter.ico"
    }
  }
}
